! Generate uplink config
interface {{ inttype }}{{ int_num }}
  description *Link_to_{{ hostname }}*
  {% if introle == 'trunk' -%}
  switchport
  switchport mode trunk
  {% elif introle == 'routed' -%}
  no switchport
  ip address {{ipaddr}} {{subnet}}
  ip ospf authentication message-digest
  ip ospf message-digest-key 1 md5 3 
  ip router ospf 10 area 0
  {% else -%}
  switchport
  switchport mode access
  switchport access vlan {{vlan}}
  {% endif -%}
  {% if portch !=None -%}
  channel-group {{ portch }} mode active
  {% endif -%}
  no shutdown
